{
  "name": "baddie-reseller",
  "version": "1.0.0",
  "description": "AI-Powered Refiller App for Digital Entrepreneurs and Resellers",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "test": "npm run test:frontend && npm run test:backend",
    "test:frontend": "cd frontend && npm test",
    "test:backend": "cd backend && npm test",
    "lint": "npm run lint:frontend && npm run lint:backend",
    "lint:frontend": "cd frontend && npm run lint",
    "lint:backend": "cd backend && npm run lint",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "clean": "rm -rf node_modules frontend/node_modules backend/node_modules",
    "setup": "npm run install:all && npm run build"
  },
  "keywords": [
    "ai",
    "automation",
    "reseller",
    "refiller",
    "ecommerce",
    "shopify",
    "stripe",
    "inventory",
    "digital-entrepreneur"
  ],
  "author": "Baddie Reseller Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "prettier": "^3.0.3",
    "eslint": "^8.50.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lovetrulymichelle-tech/Baddie-resller.git"
  },
  "bugs": {
    "url": "https://github.com/lovetrulymichelle-tech/Baddie-resller/issues"
  },
  "homepage": "https://github.com/lovetrulymichelle-tech/Baddie-resller#readme"
}